{
  "name": "snowflake-api-proxy-service",
  "version": "1.0.0",
  "description": "A comprehensive API proxy service for Snowflake Container Services with GUI management",
  "main": "backend/src/app.js",
  "scripts": {
    "start": "node backend/src/app.js",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon backend/src/app.js",
    "dev:frontend": "cd frontend && npm start",
    "build": "cd frontend && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:backend": "cd backend && npm test",
    "test:frontend": "cd frontend && npm test",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:e2e": "jest --testPathPattern=tests/e2e",
    "test:performance": "jest --testPathPattern=tests/performance",
    "test:security": "jest --testPathPattern=tests/security",
    "test:api": "jest --testPathPattern=tests/api",
    "test:snowflake": "jest --testPathPattern=tests/snowflake",
    "lint": "eslint backend/src frontend/src tests",
    "lint:fix": "eslint backend/src frontend/src tests --fix",
    "lint:backend": "eslint backend/src",
    "lint:frontend": "eslint frontend/src",
    "lint:tests": "eslint tests",
    "format": "prettier --write \"**/*.{js,jsx,json,md,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,json,md,yml,yaml}\"",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "setup": "./scripts/setup.sh",
    "health-check": "./scripts/health-check.sh",
    "deploy:production": "./scripts/deploy.sh",
    "deploy": "./scripts/deploy.sh",
    "cleanup": "./scripts/cleanup.sh",
    "cleanup:yes": "./scripts/cleanup.sh --yes",
    "test:deploy": "./scripts/test-deploy-validation.sh",
    "test:deploy:e2e": "./scripts/test-deploy-e2e.sh",
    "clean": "rm -rf node_modules frontend/node_modules backend/node_modules coverage dist",
    "clean:coverage": "rm -rf coverage",
    "clean:dist": "rm -rf dist frontend/build",
    "ci": "npm run lint && npm run test:coverage && npm run build",
    "precommit": "npm run lint && npm run test",
    "postinstall": "npm run setup"
  },
  "keywords": [
    "snowflake",
    "api-proxy",
    "container-services",
    "react",
    "nodejs",
    "express",
    "jwt",
    "pat-tokens",
    "gui-management"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.22.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.2",
    "babel-jest": "^29.7.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "jest-sonar-reporter": "^2.0.0",
    "jest-watch-typeahead": "^2.2.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0",
    "supertest": "^6.3.3"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "snowflake-sdk": "^1.9.0",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "uuid": "^9.0.1",
    "crypto": "^1.0.1"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "backend/src/**/*.js",
      "frontend/src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/coverage/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/snowflake-api-proxy-service.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/snowflake-api-proxy-service/issues"
  },
  "homepage": "https://github.com/yourusername/snowflake-api-proxy-service#readme"
}